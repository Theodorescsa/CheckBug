<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="estate_type_view_tree" model="ir.ui.view">
        <field name="name">estate_type.view.tree</field>
        <field name="model">estate_type</field>
        <field name="arch" type="xml">
            <tree string="Estate Type">
                <field name="name"/>
                <field name="buyer_id"/>
                <field name="salesperson_id"/>
            </tree>
        </field>
    </record>
    <record id="estate_type_view_form" model="ir.ui.view">
        <field name="name">estate_type.view.form</field>
        <field name="model">estate_type</field>
        <field name="arch" type="xml">
            <form string="">
                <sheet>
                    <group>
                        <field name="name"/>
                        <field name="buyer_id"/>
                        <field name="salesperson_id"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
</odoo>
