<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="estate_tags_view_tree" model="ir.ui.view">
        <field name="name">estate_tags.view.tree</field>
        <field name="model">estate_tags</field>
        <field name="arch" tags="xml">
            <tree string="Estate tags">
                <field name="name"/>
 
            </tree>
        </field>
    </record>
    <record id="estate_tags_view_form" model="ir.ui.view">
        <field name="name">estate_tags.view.form</field>
        <field name="model">estate_tags</field>
        <field name="arch" type="xml">
            <form string="">
                <sheet>
                    <group>
                        <field name="name"/>

                    </group>
                </sheet>
            </form>
        </field>
    </record>
</odoo>
